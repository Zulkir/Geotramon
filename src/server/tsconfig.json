{
  "compilerOptions": {
    "rootDir": "../",
	"target": "es2017",
    "module": "commonjs",
    "sourceMap": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "removeComments": false,
    "moduleResolution": "node",
    "outDir": "../../dist/js"
  },
  "exclude": [
    "node_modules"
  ]
}